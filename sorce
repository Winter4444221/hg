if game.PlaceId == 155615604 then
    local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua%22))()
    local Window = library.CreateLib("Police hub", "Ocean")
    local Combat = Window:NewTab("Combat")
    local Player = Window:NewTab("Player")
    local Teleporting = Window:NewTab("Teleporting")
    local Visuals = Window:NewTab("Visuals")
    local Farming = Window:NewTab("Farming")
    local PlacesSection = Teleporting:NewSection("Places")
    local CombatSection = Combat:NewSection("Combat")
    local GunsSection = Combat:NewSection("Weapons")
    local ModSection = Combat:NewSection("Mod")
    local PlayerSection = Player:NewSection("Movement")
    local CharacerSection = Player:NewSection("Player")
    local AutoSection = Farming:NewSection("Auto")
    local ESPSection = Visuals:NewSection("ESP")
    local World = Window:NewTab("World")
    local TeamSection = World:NewSection("Teams")
    local Credits = Window:NewTab("Credits")
    local ChatSection = Player:NewSection("Chat")
    local CreditsSection = Credits:NewSection("Credits")
    local PlayerGameSection = Player:NewSection("Game")
    getgenv().keycard = nil
    getgenv().Tracers = nil
    getgenv().godmode = nil
    getgenv().teamColor = nil
    getgenv().KillAura = nil
    getgenv().Names = nil
    getgenv().InfJump = nil
    CharacerSection:NewSlider("Field Of View", "Changes your FOV", 120, 60, function(fov)
        game.Workspace.Camera.FieldOfView = fov
    end)
    PlayerSection:NewSlider("Walk Speed", "Changes your walk speed", 500, 16, function(speed)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = speed
    end)
    PlayerSection:NewSlider("Jump Power", "Changes how high you jump", 500, 50, function(power)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = power
    end)
    GunsSection:NewButton("Give All Weapons", "Gives You All The Weapons", function()
local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver["AK-47"].ITEMPICKUP
        local Event = game:GetService("Workspace").Remote.ItemHandler
        Event:InvokeServer(A_1)
        local A_3 = game:GetService("Workspace")["Prison_ITEMS"].giver["Remington 870"].ITEMPICKUP
        local Event = game:GetService("Workspace").Remote.ItemHandler
        Event:InvokeServer(A_3)
        local A_2 = game:GetService("Workspace")["Prison_ITEMS"].giver["M9"].ITEMPICKUP
        local Event = game:GetService("Workspace").Remote.ItemHandler
        Event:InvokeServer(A_2)
    end)
    GunsSection:NewButton("Give AK47", "Gives You The AK47", function()
        local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver["AK-47"].ITEMPICKUP
        local Event = game:GetService("Workspace").Remote.ItemHandler
        Event:InvokeServer(A_1)
    end)
    GunsSection:NewButton("Give M9", "Gives You The M9 Pistol", function(click)
        local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver["M9"].ITEMPICKUP
        local Event = game:GetService("Workspace").Remote.ItemHandler
        Event:InvokeServer(A_1)
    end)
    GunsSection:NewButton("Give Remington 870", "Gives You The Remington 870 Shotgun", function()
        local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver["Remington 870"].ITEMPICKUP
        local Event = game:GetService("Workspace").Remote.ItemHandler
        Event:InvokeServer(A_1)
    end)
    PlayerGameSection:NewToggle("Infinite Jump", "Makes you be able to infinite jump", function(toggled)
        if toggled then
            getgenv().InfJump = true
            while getgenv().InfJump == true do
                game:GetService("UserInputService").JumpRequest:connect(function()
                    if getgenv().InfJump == true then
                        game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
end
                end)
                wait()
            end
        else
            getgenv().InfJump = false
        end
    end)
    ModSection:NewButton("Mod all guns", "Mods all the guns in your inventory", function()
        game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
        local gun1 = require(game.Players.LocalPlayer.Backpack["AK-47"].GunStates)
        gun1["MaxAmmo"] = 9999999
        gun1["Spread"] = 0
        gun1["Damage"] = 1000
        gun1["CurrentAmmo"] = 9999999
        gun1["StoredAmmo"] = 9999999
        gun1["ReloadTime"] = 0.0000000001
        gun1["AutoFire"] = true
        game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
        local gun2 = require(game.Players.LocalPlayer.Backpack["Remington 870"].GunStates)
        gun2["MaxAmmo"] = 9999999
        gun2["Spread"] = 0
        gun2["Damage"] = 1000
        gun2["CurrentAmmo"] = 9999999
        gun2["StoredAmmo"] = 9999999
        gun2["ReloadTime"] = 0.0000000001
        gun2["AutoFire"] = true
        game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
        local gun3 = require(game.Players.LocalPlayer.Backpack["M9"].GunStates)
        gun3["MaxAmmo"] = 9999999
        gun3["Spread"] = 0
        gun3["Damage"] = 1000
        gun3["CurrentAmmo"] = 9999999
        gun3["StoredAmmo"] = 9999999
        gun3["ReloadTime"] = 0.0000000001
        gun3["AutoFire"] = true
    end)
    ModSection:NewButton("Mod AK47", "Mods your AK47 (Does not work if you dont own the weapon", function()
        game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
        local gun = require(game.Players.LocalPlayer.Backpack["AK-47"].GunStates)
        gun["MaxAmmo"] = 9999999
        gun["Spread"] = 0
        gun["Damage"] = 1000
        gun["CurrentAmmo"] = 9999999
        gun["StoredAmmo"] = 9999999
        gun["ReloadTime"] = 0.0000000001
        gun["AutoFire"] = true
    end)
ModSection:NewButton("Mod Remington 870", "Mods your Remington 870 (Does not work if you dont own the weapon", function()
